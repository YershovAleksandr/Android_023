<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nam.com.mylabapplication">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".BubbleActivity" android:label="@string/app_graphicsbubbleprogram" />
        <activity android:name=".bubbleprogramxml" android:label="@string/app_graphicsbubblexml" />
        <activity android:name=".GraphicsCanvasBubble" android:label="@string/app_graphicscanvasbubble" />
        <activity android:name=".GraphicsCanvasBubbleSurfaceView" android:label="@string/app_graphicscanvasbubblesv" />
        <activity android:name=".GraphicsFrameAnimationActivity" android:label="@string/app_graphicsframeanimation" />
        <activity android:name=".GraphicsPaintActivity" android:label="@string/app_graphicspaint" />
        <activity android:name=".ShapeDrawActivity" android:label="@string/app_graphicshapedraw" />
        <activity android:name=".ShapeDrawActivityXml" android:label="@string/app_graphicshapedrawxml" />
        <activity android:name=".TransitionDrawableActivity" android:label="@string/app_graphictransitiondrawable" />
        <activity android:name=".GraphicsTweenAnimationActivity" android:label="@string/app_graphicstweenanimation" />
        <activity android:name=".ValueAnimatorActivity" android:label="@string/app_graphicsvalueanimator" />
        <activity android:name=".GraphicsViewPropertyAnimatorActivity" android:label="@string/app_graphicsviewpropertyanimator" />
        <activity android:name=".AudioVideoAudioManagerActivity" android:label="@string/app_audiovideoaudiomanager" />
        <activity android:name=".AudioRecordingActivity" android:label="@string/app_audiovideoaudiorecording" />
        <activity android:name=".AudioVideoCameraActivity" android:label="@string/app_audiovideoaudiorecording" />
        <activity android:name=".AudioVideoRingtoneManagerActivity" android:label="@string/app_ringtonemanager" />
        <activity android:name=".AudioVideoVideoPlayActivity" android:label="@string/app_videoplay" />
        <activity android:name=".MusicServiceClient" android:label="@string/app_musicservice" />
        <activity android:name=".IndicateTouchLocationActivity" android:label="@string/app_indicatetouchlocation" />

        <service android:name=".MusicService" />
    </application>

</manifest>